<h1>Listing show_artworks</h1>

<table>
  <tr>
    <th>Show</th>
    <th>Tvdb</th>
    <th>Path</th>
    <th>Type</th>
    <th>Subtype</th>
    <th>Language</th>
    <th>Rating</th>
    <th>Thumbnail</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @show_artworks.each do |show_artwork| %>
  <tr>
    <td><%= show_artwork.show_id %></td>
    <td><%= show_artwork.tvdb_id %></td>
    <td><%= show_artwork.path %></td>
    <td><%= show_artwork.type %></td>
    <td><%= show_artwork.subtype %></td>
    <td><%= show_artwork.language %></td>
    <td><%= show_artwork.rating %></td>
    <td><%= show_artwork.thumbnail %></td>
    <td><%= link_to 'Show', show_artwork %></td>
    <td><%= link_to 'Edit', edit_show_artwork_path(show_artwork) %></td>
    <td><%= link_to 'Destroy', show_artwork, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Show artwork', new_show_artwork_path %>
